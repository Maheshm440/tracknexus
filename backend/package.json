{
  "name": "tracknexus-backend",
  "version": "1.0.0",
  "description": "Backend API for TrackNexus dashboard",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-prisma.js",
